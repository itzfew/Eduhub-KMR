{
  "name": "Eduhub",
  "version": "1.1.0",
  "description": "Telegram Bot EduHub",
  "main": "src/index.ts",
  "author": "itzfew (https://t.me/itzfew)",
  "homepage": "https://github.com/itzfew/eduhub-bot",
  "dependencies": {
    "@vercel/node": "^3.2.24",
    "axios": "^1.6.8",
    "date-fns": "^3.6.0",
    "dotenv-cli": "^7.4.2",
    "firebase": "^10.14.1",
    "telegraf": "^4.16.3",
    "canvas": "^2.11.2",
    "fastest-levenshtein": "^1.0.12",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/date-fns": "^2.6.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.8.6",
    "@vercel/ncc": "^0.38.2",
    "debug": "^4.3.7",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "dev": "DEBUG=bot* dotenv -- nodemon -e ts -x ts-node src/index.ts",
    "devWindows": "@powershell -Command $env:DEBUG='bot*';dotenv -- -- nodemon -e ts -x ts-node src/index.ts",
    "build": "ncc build src/index.ts -o public -m",
    "vercel-build": "apt-get update && apt-get install -y pkg-config libcairo2-dev libjpeg-dev libpango1.0-dev libgif-dev libpng-dev && npm install && ncc build src/index.ts -o public -m",
    "prettier": "prettier --write 'src/**/*.ts'",
    "lint": "tsc --noemit"
  },
  "engines": {
    "node": "18.x"
  }
}
