  "homepage": "https://github.com/itzfew/eduhub-bot",
  "dependencies": {
    "@vercel/node": "^3.2.24",
    "axios": "^1.7.7",
    "date-fns": "^3.6.0",
    "dotenv-cli": "^7.4.2",
    "firebase": "^10.14.1",
    "telegraf": "^4.16.3",
    "fastest-levenshtein": "^1.0.16",
    "node-fetch": "^3.3.2",
    "canvas": "^2.11.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/date-fns": "^2.6.0",
