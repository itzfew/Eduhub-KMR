{
  "builds": [
    {
      "src": "public/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["public/**", "assets/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
