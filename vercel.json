{
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.ts"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/api",
      "permanent": false
    }
  ],
  "functions": {
    "api/index.ts": {
      "runtime": "vercel-node@canary"
    },
    "api/**/*.ts": {
      "runtime": "vercel-node@canary"
    }
  }
}
